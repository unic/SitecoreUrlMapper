<?xml version="1.0" encoding="utf-8" ?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="4.0">
  <UsingTask TaskName="TransformXml" AssemblyFile="$(MSBuildExtensionsPath)\Microsoft\VisualStudio\v10.0\Web\Microsoft.Web.Publishing.Tasks.dll"/>
    
    <PropertyGroup>
        <!--Setting Servertype Variable !-->
        <Servertype Condition="'$(Environment)' == ''">local</Servertype>
        <Servertype Condition="'$(Environment)' == 'Test.Author' or '$(Environment)' == 'Prod.Author' or '$(Environment)' == 'Dev.Author' or '$(Environment)' == 'Dev.Development' or '$(Environment)' == 'Dev.Test'">author</Servertype>
        <Servertype Condition="'$(Environment)' == 'Test.Delivery' or '$(Environment)' == 'Prod.Delivery' or '$(Environment)' == 'Dev.Delivery'">delivery</Servertype>
    </PropertyGroup>
    
</Project>
